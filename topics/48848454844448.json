    "talk": {
        "owner": {
            "user_id": 825812841542,
            "name": "夜息",
            "avatar_url": "https://images.zsxq.com/FvELUMsl_waKEOVTsVQQPFvUTjKQ?e=1590940799&token=kIxbL07-8jAj8w1n4s9zv64FuZZNEATmlU_Vm6zD:M-6GDCxz4KQV5JvGjvnGVnxDvs8="
